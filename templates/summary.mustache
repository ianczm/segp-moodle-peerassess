{{!
    @template mod_peerassess/summary

    Displays peerassess summary

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * none

    Example context (json):
    {
        "completedcount" : 15,
        "itemscount" : 5
    }

}}

<div class="peerassess_info">
    <span
        class="peerassess_info">{{#str}}completed_peerassesss, mod_peerassess{{/str}}: </span><span
        class="peerassess_info_value">{{completedcount}}</span>
</div>

<div class="peerassess_info">
    <span
        class="peerassess_info">{{#str}}questions, mod_peerassess{{/str}}: </span><span
        class="peerassess_info_value">{{itemscount}}</span>
</div>
